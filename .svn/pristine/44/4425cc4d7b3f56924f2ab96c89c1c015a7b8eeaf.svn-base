/* common start */
body { _margin:0; _height:100%; /*IE6 BUG*/ }
.aui_outer { text-align:left; }
table.aui_border, table.aui_dialog { border:0; margin:0; border-collapse:collapse; width:auto; }
.aui_nw, .aui_n, .aui_ne, .aui_w, .aui_c, .aui_e, .aui_sw, .aui_s, .aui_se, .aui_header, .aui_tdIcon, .aui_main, .aui_footer { padding:0; }
.aui_header, .aui_buttons button { font: 14px/1.11 'Microsoft Yahei', Tahoma, Arial, Helvetica, STHeiti; _font-family:Tahoma,Arial,Helvetica,STHeiti; -o-font-family: Tahoma, Arial; }
.aui_title { overflow:hidden; text-overflow: ellipsis; }
.aui_state_noTitle .aui_title { display:none; }
.aui_close { display:block; position:absolute; text-decoration:none; outline:none; _cursor:pointer; }
.aui_close:hover { text-decoration:none; }
.aui_main { text-align:center; min-width:9em; min-width:0\9/*IE8 BUG*/; }
.aui_main .aui_iconBg { width: 32px;height: 32px;margin: 0 auto;margin-bottom: 20px;background-position: center center;background-repeat: no-repeat; }
.aui_main .prompt_success { background: url(icons/success.png);}
.aui_main .prompt_error { background: url(icons/error.png);}
.aui_content { display:inline-block; *zoom:1; *display:inline; text-align:center; border:none 0; }
.aui_content.aui_state_full { display:block; width:100%; margin:0; padding:0!important; height:100%; }
.aui_loading { width:96px; height:32px; text-align:left; text-indent:-999em; overflow:hidden; background:url(icons/loading.gif) no-repeat center center; }
.aui_icon { vertical-align: middle; }
.aui_icon div { width:32px; height:32px; margin:10px 0 10px 20px; background-position: center center; background-repeat:no-repeat; }
.aui_buttons { padding:12px 17px 10px; text-align:center; white-space:nowrap; }
.aui_buttons button {outline:none; margin-left:15px; background:#c1c1c1; border-radius:5px; cursor: pointer; display: inline-block; text-align: center; *padding:4px 10px; *height:2em; *width:1; color: #ffffff; border: solid 1px #c1c1c1; min-width:70px;height:32px; line-height:32px;}
.aui_buttons button::-moz-focus-inner{ border:0; padding:0; margin:0; }
.aui_buttons button:hover { color:#ffffff; background:#ABAAAA; border:1px solid #ABAAAA; }
.aui_buttons button:active { }
.aui_buttons button[disabled] { cursor:default; color:#666; background:#DDD; border: solid 1px #ccc; }
button.aui_state_highlight { border-radius:5px; color: #FFF;  background:#3390ff; width:70px;height:32px; line-height:32px; border: none;}
button.aui_state_highlight:hover {background:#489bff;color: #FFF;border: none;}
/* common end */

.aui_inner { background:#FFF;border:1px solid #ccc; }
.aui_outer, .aui_inner { border-radius: 5px 5px 5px 5px!important; }
.aui_border { }
.aui_nw, .aui_ne, .aui_sw, .aui_se { width:8px; height:8px; }
.aui_nw, .aui_n, .aui_ne, .aui_w, .aui_e, .aui_sw, .aui_s, .aui_se {}
.aui_state_lock .aui_nw, .aui_state_lock .aui_n, .aui_state_lock .aui_ne, .aui_state_lock .aui_w, .aui_state_lock .aui_e, .aui_state_lock .aui_sw, .aui_state_lock .aui_s, .aui_state_lock .aui_se {  }
.aui_state_focus .aui_dialog {  }
.aui_state_focus .aui_outer {  }
.aui_state_lock .aui_border { }
.aui_state_drag .aui_outer, .aui_outer:active { box-shadow:none; }
.aui_titleBar { position:relative; height:100%; }
.aui_title { height:50px; line-height:50px; padding:0 28px 0 20px; color:#333; font-family: Microsoft Yahei, arial/9!important; border-bottom:1px solid #ccc; }
.aui_state_focus .aui_title { color:#676A6C; font-size:14px; font-family: "microsoft yahei";}
.aui_state_drag .aui_title { }
.aui_state_drag .aui_titleBar { box-shadow:none; }
.aui_close { padding:0; right:14px; width:21px; height:40px; top:16px; font-size:18px; color:#68767b; text-align:center; font-family: Microsoft Yahei, arial;  }
a.aui_close:focus{text-decoration: none;}
.aui_close:hover { color:#C72015; }
.aui_content { color:#666; }
.aui_state_focus .aui_content { color:#000; /* min-width: 160px */}
.aui_buttons { background-color:#Fff; border-top:solid 1px #ddd; }
.aui_state_noTitle .aui_nw, .aui_state_noTitle .aui_ne, .aui_state_noTitle .aui_sw, .aui_state_noTitle .aui_se { width:3px; height:3px; }
.aui_state_noTitle .aui_inner { border:1px solid #666; background:#FFF; border-radius:5px;}
.aui_state_noTitle .aui_outer { border:none 0; box-shadow:none; }
.aui_state_noTitle .aui_nw, .aui_state_noTitle .aui_n, .aui_state_noTitle .aui_ne, .aui_state_noTitle .aui_w, .aui_state_noTitle .aui_e, .aui_state_noTitle .aui_sw, .aui_state_noTitle .aui_s, .aui_state_noTitle .aui_se { background:rgba(0, 0, 0, .05); background:#000\9!important; filter:alpha(opacity=5)!important; }
.aui_state_noTitle .aui_titleBar { bottom:0; _bottom:0; _margin-top:0; }
.aui_state_noTitle .aui_close { top:0; right:0; width:18px; height:18px; line-height:18px; text-align:center; text-indent:0; font-size:18px; text-decoration:none; color:#214FA3; background:none; filter:!important; }
.aui_state_noTitle .aui_close:hover, .aui_state_noTitle .aui_close:active { text-decoration:none; color:#900; }
.aui_state_noTitle .aui_dialog { box-shadow: none; }